{
  "name": "timer_app",
  "version": "1.0.0",
  "author": "Fullstack.io",
  "scripts": {
    "go": "open http://localhost:3000; npm run server",
    "e2e": "nightwatch; npm run reset-data;",
    "reset-data": "cp data.json.pristine data.json",
    "test": "npm run reset-data; ./node_modules/.bin/concurrently -k 'npm run server' 'npm run e2e'",
    "server": "./node_modules/.bin/babel-node server.js",
    "build": "webpack",
    "stats": "webpack --profile --json > stats.json",
    "deploy": "gh-pages -d build",
    "start": "webpack-dev-server"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "private": true,
  "dependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.4.5",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "^6.16.0",
    "body-parser": "1.14.1",
    "express": "4.13.3",
    "file-loader": "^0.9.0",
    "fs": "0.0.2",
    "lodash": "^4.15.0",
    "path": "0.12.7",
    "react": "^15.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.3.13",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "2.2.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.11.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "jshint": "^2.9.3",
    "jshint-loader": "^0.8.3",
    "purecss": "^0.6.0",
    "purifycss-webpack-plugin": "^2.0.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  }
}
